CREATE TABLE IF NOT EXISTS competing_univeristy(
  name VARCHAR(200) NOT NULL,   /* School Name */
  date DATE NOT NULL DEFAULT CURRENT_DATE,
  address TEXT,             /* Address*/
  population INTEGER NOT NULL,            /* Population */
  rate DECIMAL,      /* acceptance rate  */ 
  PRIMARY KEY (name)
);

INSERT INTO competing_univeristy(name, date, address, population, rate)
VALUES('CU Boulder', to_date('1876', 'yyyy'), '1100 28th St, Boulder, CO 80303', 35000, 80);